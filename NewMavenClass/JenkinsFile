pipeline{
  agent any 
    tools {
      maven 'maven'
      }
     
    
  
  stages {
    stage('Build'){
      steps{
        bat 'mvn -f CodingChallengeMaven\pom.xml install'
      }
     }
    stage('Test'){
      steps{
         bat 'mvn -f CodingChallengeMaven\pom.xml test'
      }
      
  }
  }
}
