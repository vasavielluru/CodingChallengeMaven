pipeline{
  agent any 
    tools {
      maven 'Maven'
      jdk 'jdk8'
      }
     
    
  
  stages {
  stage ('Initialize') {
            steps {
                echo "Pipeline started"
            }
        }
        stage('compile') {
        steps{
             bat 'mvn -f NewMavenClass/pom.xml clean compile'
        }
        }
    stage('Build'){
      steps{
       
       sh 'mvn -f NewMavenClass/pom.xml clean install'
      }
       post {
                success {
                    bat 'mvn -f NewMavenClass/pom.xml test' 
                }
            }
     }
   
  }
}
